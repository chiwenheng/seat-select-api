<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
  <classPathEntry location="/Users/zenghao/Documents/GitHub/seat/jar/mysql-connector-java-8.0.16.jar"/>

  <context id="context1" >
  	
  	<commentGenerator>
	    <property name="javaFileEncoding" value="UTF-8"/>
        <property name="suppressDate" value="true"/>
        <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
        <property name="suppressAllComments" value="true"/>
	</commentGenerator> 
    <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
    				connectionURL="jdbc:mysql://localhost:3306/seat_select"
    				userId="root"
    				password="123456" />
    <javaModelGenerator targetPackage="com.zh.seat.model" targetProject="src/main/java">
     <property name="enableSubPackages" value="true"/>
     <property name="trimStrings" value="true"/>
	</javaModelGenerator>
    <sqlMapGenerator targetPackage="mapping" targetProject="src/main/resources">
    	<property name="enableSubPackages" value="true"/>
    </sqlMapGenerator>
    <javaClientGenerator targetPackage="com.zh.seat.mapper" targetProject="src/main/java" type="XMLMAPPER" >
    <property name="enableSubPackages" value="true"/>
    </javaClientGenerator>
    
    <table tableName="ss_seat_templete" domainObjectName="SeatTemplete"
           enableCountByExample="false"
           enableUpdateByExample="false"
           enableDeleteByExample="false"
           enableSelectByExample="false"
           enableInsert="false"
           selectByExampleQueryId="false">
    </table>
  </context>
</generatorConfiguration>